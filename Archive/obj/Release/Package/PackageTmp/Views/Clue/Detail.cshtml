@using Model;
@{
    Layout = null;
    ClueLink item = ViewBag.cluelink;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title></title>
    <script src="~/Resource/edit.js"></script>
   @* <link href="~/Resource/form.css" rel="stylesheet" />*@
    <script src="~/Web/guid.js"></script>
    @RenderPage("~/Views/Share/bootstrap.cshtml")
</head>
<body>
    <div class="row" style="margin-top:20px;">
        <div class="col-xs-1">
            <input name="b_print" type="button" class="ipt btn btn-primary " onclick="printdiv('div_print');" value="打印">
        </div>
    </div>
    <div style="" id="div_print">
        <form id="Clue" name="Clue" method="post" class="form-inline">
            <div class="row">
                <input readonly style="display:none" type="text" value="@item.ID" id="ID" name="ID" />
                <div class="col-md-12">
                    <h3 class="Gradient">信访举报</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="HandleDeptName">处理部门：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class=" form-control" type="text" id="HandleDeptName" name="HandleDeptName" value="@item.HandleDeptName" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="HandleStaffName"> 处理人员：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="HandleStaffName" name="HandleStaffName" class="form-control" value="@item.HandleStaffName" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="Status">线索状态：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="Status" name="Status" class="form-control" value="@item.Status" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <h3 class="">线索登记</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueCode">信访编号：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class=" form-control" type="text" id="ClueCode" name="ClueCode" value="@item.ClueCode" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="BarCode"> 条形码：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="BarCode" name="BarCode" class="form-control" value="@item.BarCode" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueMode">信访方式：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ClueMode" name="ClueMode" class="form-control" value="@item.ClueMode" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueDate">信访日期：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class=" form-control " name="ClueDate" value="@item.ClueDate">
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueType"> 信访类别：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ClueType" name="ClueType" class="form-control" value="@item.ClueType" />

                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueSource">信访来源：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ClueSource" name="ClueSource" class="form-control" value="@item.ClueSource" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueNum">信访人数：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ClueNum" name="ClueNum" value="@item.ClueNum" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="Urgency"> 紧急程度：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="Urgency" name="Urgency" class="form-control" value="@item.Urgency" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <p style="font-size: 14px; font-family: helvetica neue;font-weight:600">被反映对象</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="HandleType"> 被反映对象类型：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ObjType" name="ObjType" class="form-control" value="@item.ObjType" />

                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjName"> 姓名：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ObjName" name="ObjName" value="@item.ObjName" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjAddress"> 单位或地址：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ObjAddress" name="ObjAddress" value="@item.ObjAddress" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjType"> 行政级别：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ObjAdminLvl" name="ObjAdminLvl" class="form-control" value="@item.ObjAdminLvl" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjComMode">单位性质：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ObjComMode" name="ObjComMode" value="@item.ObjComMode" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjArea">所属区域：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ObjArea" name="ObjArea" value="@item.ObjArea" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjPost">职务：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly type="text" id="ObjPost" name="ObjPost" class="form-control" value="@item.ObjPost" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjControler"> 监察对象：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly type="text" id="ObjControler" name="ObjControler" class="form-control" value="@item.ObjControler" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjIsManager">是否负责人：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly type="text" id="ObjIsManager" name="ObjIsManager" class="form-control" value="@item.ObjIsManager" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjIsLeader">是否本级干部：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ObjIsLeader" name="ObjIsLeader" class="form-control" value="@item.ObjIsLeader" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjCardNum"> 身份证号：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ObjCardNum" name="ObjCardNum" value="@item.ObjCardNum" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjSex">性别：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ObjSex" name="ObjSex" class="form-control" value="@item.ObjSex" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjBirthday">出生日期：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                        <input readonly class=" form-control " name="ObjBirthday" value="@item.ObjBirthday">
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ObjNation">民族：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ObjNation" name="ObjNation" value="@item.ObjNation" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <p style="font-size: 14px; font-family: helvetica neue;font-weight:600">反映人</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectIsSign">是否署名：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ReflectIsSign" name="ReflectIsSign" class="form-control" value="@item.ReflectIsSign" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectName"> 姓名：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectName" name="ReflectName" value="@item.ReflectName" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectAddress">单位或地区：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectAddress" name="ReflectAddress" value="@item.ReflectAddress" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectAdminLvl"> 行政级别：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ReflectAdminLvl" name="ReflectAdminLvl" class="form-control" value="@item.ReflectAdminLvl" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectLinkPhone">联系电话：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectLinkPhone" name="ReflectLinkPhone" value="@item.ReflectLinkPhone" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectArea"> 所属区域：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectArea" name="ReflectArea" value="@item.ReflectArea" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectPost">职务：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectPost" name="ReflectPost" value="@item.ReflectPost" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectEmail"> 电子邮箱：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectEmail" name="ReflectEmail" value="@item.ReflectEmail" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectZipCode">邮政编码：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectZipCode" name="ReflectZipCode" value="@item.ReflectZipCode" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectTxaddress">通讯地址：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectTxaddress" name="ReflectTxaddress" value="@item.ReflectTxaddress" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectNation"> 民族：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectNation" name="ReflectNation" value="@item.ReflectNation" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectSex">性别：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ReflectSex" name="ReflectSex" class="form-control" value="@item.ReflectSex" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectPolitics"> 政治面貌：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="ReflectPolitics" name="ReflectPolitics" class="form-control" value="@item.ReflectPolitics" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectVocation"> 职业：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectVocation" name="ReflectVocation" value="@item.ReflectVocation" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReflectCardNumr">身份证号：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="ReflectCardNumr" name="ReflectCardNumr" value="@item.ReflectCardNumr" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <p style="font-size: 14px; font-family: helvetica neue;font-weight:600">反映其他问题</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="MainMode">问题类别：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="MainMode" name="MainMode" class="form-control" value="@item.MainMode" />
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="BelongAddress">所属地区：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="BelongAddress" name="BelongAddress" value="@item.BelongAddress" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="KeyWord"> 关键词：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="KeyWord" name="KeyWord" value="@item.KeyWord" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="Des">问题描述：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="Des" name="Des" class=" margin-5 border w-adapt-text">@item.Des</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="Remark">问题备注：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="Remark" name="Remark" class=" margin-5 border w-adapt-text">@item.Remark</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReplyNote">回复内容：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="ReplyNote" name="ReplyNote" class=" margin-5 border w-adapt-text">@item.ReplyNote</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <h3 class="">线索呈批</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="FillInDate">呈批时间：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <div class="form-group">
                        <input readonly class=" form-control " name="FillInDate" value="@item.FillInDate">
                    </div>
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="Undertaker">承办人：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly class="form-control" type="text" id="Undertaker" name="Undertaker" value="@item.Undertaker" />
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="DeviseOpinion">拟办意见：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="DeviseOpinion" name="DeviseOpinion" class=" margin-5 border w-adapt-text">@item.DeviseOpinion</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="DeptOpinion">部门意见：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="DeptOpinion" name="DeptOpinion" class=" margin-5 border w-adapt-text">@item.DeptOpinion</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ReplyNote">领导批示：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="ReplyNote" name="ReplyNote" class=" margin-5 border w-adapt-text">@item.ReplyNote</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <h3 class="">线索处置</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="SaveDate">处置时间：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <div class="form-group">
                        <input readonly class=" form-control " name="SaveDate" value="@item.SaveDate">
                    </div>
                </div>
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="HandleType"> 处置方式：</label>
                </div>
                <div class="col-md-2 margin-top-10">
                    <input readonly id="HandleType" name="HandleType" class="form-control" value="@item.HandleType" />
                </div>
            </div>
            @{if (@item.HandleType == "拟立案")
{
                <div class="row">
                    <div class="col-md-2 margin-top-10">
                        <label class="margin-5" for="ViolateDate">违纪时间：</label>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <div class="form-group">
                            <input readonly class=" form-control " name="ViolateDate" value="@item.ViolateDate">
                        </div>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <label class="margin-5" for="CrimeDate">案发时间：</label>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <div class="form-group">
                            <input readonly class=" form-control " name="CrimeDate" value="@item.CrimeDate">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 margin-top-10">
                        <label class="margin-5" for="UndertakeDate">承办时间：</label>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <div class="form-group">
                            <input readonly class=" form-control " name="UndertakeDate" value="@item.UndertakeDate">
                        </div>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <label class="margin-5" for="HandleSource"> 线索来源：</label>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <input readonly class="form-control" type="text" id="HandleSource" name="HandleSource" value="@item.HandleSource" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 margin-top-10">
                        <label class="margin-5" for="QuestionNature"> 问题性质：</label>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <input readonly class="form-control" type="text" id="QuestionNature" name="QuestionNature" value="@item.QuestionNature" />
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <label class="margin-5" for="CrimeMoney"> 违纪数额（万元）：</label>
                    </div>
                    <div class="col-md-2 margin-top-10">
                        <input readonly class="form-control" type="text" id="CrimeMoney" name="CrimeMoney" value="@item.CrimeMoney" />
                    </div>
                </div>
}}
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="BasicInfo">个人基本情况：</label>
                </div>

                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="BasicInfo" name="BasicInfo" class=" margin-5 border w-adapt-text">@item.BasicInfo</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
                    <label class="margin-5" for="ClueSourceAndMainProblem">线索来源和反映的主要问题：</label>
                </div>
                <div class="col-md-10 margin-top-10">
                    <textarea readonly rows="5" id="ClueSourceAndMainProblem" name="ClueSourceAndMainProblem" class=" margin-5 border w-adapt-text">@item.ClueSourceAndMainProblem</textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 margin-top-10">
